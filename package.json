{
  "name": "KoL-Mafioso",
  "version": "4.5.1",
  "description": "A visualizer for kolmafia logs for the game Kingdom of Loathing.",
  "url": "https://github.com/aahvocado/KoL-Mafioso",
  "email": "dan.dan.makes.stuff@gmail.com",
  "license": "MIT",
  "scripts": {
    "sass.compile": "sass src/styles/main.scss src/styles/compiled/main.css",
    "sass.watch": "sass --watch src/styles/main.scss src/styles/compiled/main.css",
    "test": "react-scripts test",
    "build": "npm run sass.compile && REACT_APP_VERSION=$npm_package_version react-scripts build",
    "start": "npm run sass.compile && REACT_APP_VERSION=$npm_package_version react-scripts start"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.2",
    "base-64": "^0.1.0",
    "chart.js": "^2.9.3",
    "mobx": "^5.15.4",
    "mobx-react": "^6.2.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-dropdown-date": "^2.2.4",
    "react-meta-tags": "^1.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "5.0.1",
    "tinygradient": "^1.1.2",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "aah-util-css": "^0.5.3",
    "sass": "^1.26.10"
  }
}
